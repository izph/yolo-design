{
  "name": "yolo-ui",
  "version": "0.1.0",
  "private": false,
  "description": "A React components library",
  "title": "YoLo UI",
  "author": "iZph",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.js",
  "keywords": [
    "yolo-ui",
    "component",
    "design",
    "frontend",
    "react",
    "react-component",
    "ui"
  ],
  "homepage": "https://github.com/izph/yolo-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/izph/yolo-ui"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "dumi dev",
    "clean": "rimraf ./dist",
    "build:lib": "npm run clean && npm run build-ts && npm run build-css",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "lessc ./src/style/index.less ./dist/index.css",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint --ext ts,tsx,js src",
    "lint:stylelint": "stylelint **/*.{html,css,less} --fix",
    "test:coverage": "umi-test --coverage",
    "prepublishOnly": "npm run build:lib"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "peerDependencies": {
    "react": "≥16.8.0",
    "react-dom": "≥16.8.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "axios": "^0.26.1",
    "classnames": "^2.2.6",
    "react-transition-group": "^4.4.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^27.0.3",
    "@types/node": "^12.19.9",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "dumi": "^1.0.10",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-react-hooks": "^4.1.2",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3",
    "yorkie": "^2.0.0"
  }
}